type Services {
    search(filter: ServicesFilter): ServicesSearchResult!
    add(entry: ServiceEntry) : ServiceModel!
}

input ServiceEntry {
    whenProvided: String
    forWhatCustomer: ID
}

type ServicesSearchResult {
    items: [ServiceModel!]!
    totalDistance: Int!
}

type ServiceModel {
    whenProvided: String
    forWhatCustomer: String
}

input ServicesFilter {
    whenProvided: String
    forWhatCustomer: String
    onlyMine: Boolean
}
