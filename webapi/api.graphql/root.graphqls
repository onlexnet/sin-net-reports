type Subscription {
    time: Int
}

input ServiceEntry {
    whenProvided: String
    forWhatCustomer: ID
}

type PrincipalModel {
    name: String
}

# The Root Query for the application
type Query {
    getPrincipal: PrincipalModel!
    Services: Services!
}
 
# The Root Mutation for the application
type Mutation {
    addService(entry: ServiceEntry) : ServiceModel!
}