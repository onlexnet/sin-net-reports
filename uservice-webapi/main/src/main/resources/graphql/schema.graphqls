  entityCode: String
  entityName: String
  location: String!
  password: String
  username: String
}

type CustomersMutation {
  projectId: String
  remove(id: MyEntity!): Boolean!
  reserve: SomeEntity!
  save(contacts: [CustomerContactInput!]!, entry: CustomerInput!, id: MyEntity!, secrets: [CustomerSecretInput!]!, secretsEx: [CustomerSecretExInput!]!): SomeEntity!
}

type CustomersQuery {
  get(entityId: ID): CustomerEntity
  list: [CustomerEntity!]!
  projectId: String
}

"Mutation root"
type Mutation {
  Actions(projectId: ID!): ActionsMutation!
  Customers(projectId: ID!): CustomersMutation!
  Projects: ProjectsMutation!
}

type PrincipalModel {
  name: String
}

type ProjectEntity {
  entity: SomeEntity
  name: String!
}

type ProjectIdModel {
  id: ID!
  tag: Long!
}

type ProjectMutation {
  withOperator(operatorEmail: String!): ProjectIdModel!
}

type ProjectsMutation {
  project(eid: ID!, etag: ID!): ProjectMutation!
  remove(projectId: ProjectIdInput!): Boolean!
  save(name: String!): ProjectEntity!
}

type ProjectsQuery {
  list(name: String!): [ProjectEntity]!
  numberOfProjects: Int!
}

"Query root"
type Query {
  Actions(projectId: ID!): ActionsQuery!
  Customers(projectId: ID!): CustomersQuery!
  Projects: ProjectsQuery!
  Users(projectId: ID!): UsersQuery!
  getPrincipal: PrincipalModel!
  test: PrincipalModel!
}

type ServiceModel {
  customer: CustomerEntity
  description: String
  distance: Int
  duration: Int
  entityId: ID!
  entityVersion: Long!
  projectId: ID!
  servicemanEmail: String
  servicemanName: String
  "ISO-8601"
  whenProvided: Date!
}

type ServicesSearchResult {
  items: [ServiceModel!]!
}

type SomeEntity {
  entityId: ID!
  entityVersion: Long!
  projectId: ID!
}

type User {
  email: String!
  entityId: ID!
}

type UsersQuery {
  projectId: String
